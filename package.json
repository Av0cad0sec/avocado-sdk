{
  "name": "@instadapp/avocado",
  "version": "0.4.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/avocado.cjs",
  "module": "./dist/avocado.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/avocado.js",
      "require": "./dist/avocado.cjs"
    }
  },
  "scripts": {
    "generate:contracts": "rimraf src/contracts && typechain --target=ethers-v5 'abi/*.json' --out-dir 'src/contracts'",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "yarn && npm run build"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/solidity": "^5.7.0",
    "@ethersproject/strings": "^5.7.0",
    "@vue/runtime-dom": "^3.2.45",
    "@web3-react/abstract-connector": "^6.0.7",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "mitt": "^3.0.0",
    "semver": "^7.3.8",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/semver": "^7.3.13",
    "@vitejs/plugin-vue": "^4.0.0",
    "@web3-react/types": "^6.0.7",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11"
  }
}
